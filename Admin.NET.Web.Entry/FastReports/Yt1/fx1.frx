    if (iLen &gt; 0)
    {
    double fSize = celTmp.Width / iLen;

    if (fSize &gt; 5.66929)
    iSize = 8;
    else if (fSize &gt; 4.53543)
    iSize = 7;
    else if (fSize &gt; 4.15748)
    iSize = 6;
    else
    iSize = 5;
    Font font = new Font(&quot;宋体&quot;, iSize);
    celTmp.Font = font;
    }
    }
    }
    }
